# OpenClaw Configuration Directory
# This directory stores OpenClaw configuration files
# Will be created automatically if it doesn't exist
OPENCLAW_CONFIG_DIR=./openclaw/config

# OpenClaw Workspace Directory
# This directory is used as the working directory for OpenClaw operations
# Will be created automatically if it doesn't exist
OPENCLAW_WORKSPACE_DIR=./openclaw/workspace

# OpenClaw Gateway Token
# Set a secure token for gateway authentication
# Generate with: openssl rand -hex 32
OPENCLAW_GATEWAY_TOKEN=

# OpenClaw Gateway Port (default: 18789)
OPENCLAW_GATEWAY_PORT=18789

# OpenClaw Bridge Port (default: 18790)
OPENCLAW_BRIDGE_PORT=18790

# OpenClaw Gateway Bind Address
# Options: lan, localhost
OPENCLAW_GATEWAY_BIND=lan

# Claude AI Session Key
# Get this from your Claude AI session
CLAUDE_AI_SESSION_KEY=

# Claude Web Session Key
# Get this from your Claude Web session
CLAUDE_WEB_SESSION_KEY=

# Claude Web Cookie
# Get this from your Claude Web session cookies
CLAUDE_WEB_COOKIE=
